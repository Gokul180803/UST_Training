
public class ScoreAnalysis2 {

	public static void main(String[] args) {
		int seasonMatchScores[][]= {{180}, {165}, {205}, {190}};
		int Total=0;
		for (int i=0;i<seasonMatchScores.length;i++) {
			Total+=seasonMatchScores[i][0];
		}
		double avg=Total/seasonMatchScores.length;
		
		
	System.out.println("Total Score is : "+Total);
	System.out.println("Average Score is : "+avg);
	}

}
