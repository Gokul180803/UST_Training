class Item{
	int itemId;
    double itemPrice;
	boolean isAvailable;
	String itemName;
	char categoryCode;
}
public class DefaultValue {

	public static void main(String[] args) {
		Item myItem=new Item();
		System.out.println(myItem.itemId);
		System.out.println(myItem.itemPrice);
		System.out.println(myItem.isAvailable);
		System.out.println(myItem.itemName);
		System.out.println(myItem.categoryCode);

	}

}
