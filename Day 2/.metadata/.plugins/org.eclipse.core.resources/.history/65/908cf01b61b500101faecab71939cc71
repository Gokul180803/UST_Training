package DataTypes1;
class box{
	public int size;
}
public class AssignedValue {
	public static void changeReference(box box3) {
		box3.size=500;
		System.out.println(box3);
	}
public static void main(String[]args) {
	box box1=new box();
	box1.size=100;
	box box2=new box();
	box2.size=box1.size;
	box2.size=200;
	
	changeReference(box1);
}
}
