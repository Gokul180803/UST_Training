
public class LoopExploration {

	public static void main(String[] args) {
//	for(int i=10;i>0;i--) {
//		System.out.println(i);
//	}
//	System.out.println("Blast Off!");

	       int a=1,sum=0;
	       
      while(a<=20) {
    	  if(a%2==0) {
    		  sum+=a;
    	  }
    	  a++;
    	 
      }
      System.out.println(sum);
      
}
}
