import java.util.Scanner;

public class Factorial {
	public static int fac(int num) {
		if(num==0) {
			return 0;
		}else if(num==1) {
			return 1;
		}else {
		return	fac(num)*fac(num-1);
		}
	}

	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		int a=sc.nextInt();
		System.out.print(fac(a));

	}

}
