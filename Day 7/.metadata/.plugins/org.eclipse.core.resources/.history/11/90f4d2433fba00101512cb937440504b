
public class StarPattern {

	public static void main(String[] args) {
		int a=10;
		for(int i=1;i<a;i++) {
			for(int j=a;j>=i;j--) {
			System.out.print(" ");	
			}
			for(int l=0;l<1;l++) {
				System.out.print("*");	

			}
			for(int k=2;k<=2*i-1;k++) {
				System.out.print("#");	
			}
			for(int l=0;l<1;l++) {
				System.out.print("*");	

			}
			System.out.println();
		}
//		for(int i=a;i>0;i--) {
//			for(int j=a;j>=i;j--) {
//			System.out.print(" ");	
//			}
//			for(int k=1;k<=2*i-1;k++) {
//				System.out.print("*");	
//			}
//			System.out.println();
//		}

	}

}
